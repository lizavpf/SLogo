public class ForwardTest {
	
	JTurtle turtle = new JTurtle();

	@Test
	public void testForward() {
		turtle.move(50);
		assertEquals(50.0, turtle.getPoint().getY(), 0.01);
		assertEquals(0, turtle.getPoint().getX(), 0.01);
	}



}
